{% extends 'layouts/base.html' %}
{% block title %} {{title|capitalize}} {% endblock %}
{% block content %}
{{parent()}}
<div class="panel panel-primary">
	<div class="panel-heading">
		<h3 class="panel-title text-center">{{title|upper}}</h3>
	</div>
	<!-- Table -->
	<div class="table-responsive">
		<table class="table table-striped table-hover">
			<thead>
				<tr>
					<th>Codigo</th>
					<th>Apellido paterno</th>
					<th>Nombre(s)</th>
					<!-- <th>Apellido materno</th> -->
					<th>Detalles</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<th>0</th>
					<td>1</td>
					<td>2</td>
					<!-- <td>3</td> -->
					<td class='text-left'><a href="" target='_top'><span class="fa fa-external-link"></span></a></td>
				</tr>
			</tbody>
		</table>
	</div>
</div>
{% endblock %}
