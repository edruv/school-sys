{% extends 'layouts/base.html' %}
{% block title %} {{title|capitalize}} {% endblock %}

{% block content %}
{{parent()}}
<form action="" class="text-center fad col-md-6 col-md-offset-3">
	<div class="form-group">
		<label for="npro">Nombre</label>
		<input type="text" name="npro" id="npro" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="appp">Apellido Paterno</label>
		<input type="text" name="appp" id="appp" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="apmp">Apellido Materno</label>
		<input type="text" name="apmp" id="apmp" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="empr">Correo electronico</label>
		<input type="email" name="empr" id="empr" class="form-control" placeholder="">
	</div>
	<input type="submit" value="{{title|capitalize}}" class="btn btn-success">
</form>
{% endblock %}
