{% extends 'layouts/base.twig' %}
{% block title %} {{title|capitalize|title}} {% endblock %}

{% block content %}
{{parent()}}
<form action="" class="text-center fad col-md-6 col-md-offset-3">
	<div class="form-group">
		<label for="nstu">Nombre</label>
		<input type="text" name="nstu" id="nstu" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="apps">Apellido Paterno</label>
		<input type="text" name="apps" id="apps" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="apms">Apellido Materno</label>
		<input type="text" name="apms" id="apms" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="">Correo electronico</label>
		<input type="email" name="" id="" class="form-control" placeholder="">
	</div>
	<div class="form-group">
		<label for="cast">Carrera</label>
		<select name="cast" id="cast" class="form-control text-center">
			<option value="">Seleccionar una opcion</option>
			<option value="">1</option>
			<option value="">2</option>
			<option value="">3</option>
			<option value="">4</option>
		</select>
	</div>
	<input type="submit" value="{{title|capitalize|title}}" class="btn btn-success">
</form>
{% endblock %}
