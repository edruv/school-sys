	<body>
		<div class="container">
			<form class="form-signin" method="post">
				<h2 class="form-signin-heading">Ingresa al sistema</h2>
				<label for="inputEmail" class="sr-only">Codigo</label>
				<input type="text" id="inputEmail" class="form-control" placeholder="Codigo" required autofocus>
				<label for="inputPassword" class="sr-only">Contraseña</label>
				<input type="password" id="inputPassword" class="form-control" placeholder="Contraseña" required>
				<button class="btn btn-lg btn-primary btn-block" name="signin" type="submit">Entrar</button>
			</form>
